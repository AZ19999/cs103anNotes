<h2> Exam 8b: Large CS courses since 2004 </h2>
	<table class="table table-bordered table-striped">
		<thead>
			<tr>
				<th>
					Course
				</th>
				<th>
					Semester
				</th>
				<th>
					Instructor
				</th>
				<th>
					title
				</th>
				<th>
					Enrollment
				</th>
			</tr>
		</thead>
		<tbody>
			<% for (course of courses){ %>
			<tr>
				<td>
					<%= course['subj'] + course['num'] %>
				</td>
				<td>
					<%= course['term'] %>
				</td>
				<td>
					<% = course['inst'] %>
				</td>
				<td>
					<% = course['title'] %>
				</td>
				<td>
					<% = course['enr'] %>
				</td>
			</tr>
			<% } %>
	</tbody
	</table>